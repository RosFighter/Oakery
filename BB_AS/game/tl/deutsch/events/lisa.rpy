# game/events/lisa.rpy:687
translate deutsch lisa_bath_open_bb4cd26f:

    # Lisa_12 "Макс, выйди немедленно!"
    Lisa_12 "Max, komm sofort raus!"

# game/events/lisa.rpy:688
