translate chinese olivia_repeat_sunscreen_end_be248b37:

    # Lisa_02 "Мы тебе очень благодарны, Макс. Прекрасный массаж!"
    Lisa_02 "非常感谢你，麦克斯。很棒的按摩！"

# game/dialogues/oliviatalk.rpy:738
translate chinese olivia_repeat_sunscreen_end_2909abdc:

    # Olivia_02 "Да, спасибо тебе!"
    Olivia_02 "好的，谢谢！"

