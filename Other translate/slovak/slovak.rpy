translate slovak gift_pajamas_a174e7cf_2:

    # Alice_02 "Вот и молодец! Гуляй..."
    Alice_02 "Dobre pre teba! Padaj..."

# game/dialogues/alicetalk.rpy:2607
translate slovak Alice_solar_877a92ab:

    # Alice_02 "Как ты догадался, Шерлок?" nointeract
    Alice_02 "Ako ste to uhádli, Sherlock?" nointeract

# game/dialogues/alicetalk.rpy:2609
translate slovak Alice_solar_8dc19101:

    # Alice_04 "Достаточно на сегодня, Макс..."
    Alice_04 "Pre dnešok to stačí, Max..."

# game/dialogues/alicetalk.rpy:2610
translate slovak Alice_solar_727a2390:

    # Max_00 "Ясно. Ну, тогда, может, завтра..."
    Max_00 "Chápem. Tak možno zajtra..."

# game/dialogues/alicetalk.rpy:2614
translate slovak Alice_solar_0e1005c0:

    # Alice_13 "Может быть. Вот только у меня его нет..."
    Alice_13 "Možno. Až na to, že ho nemám..."

# game/dialogues/alicetalk.rpy:2615
translate slovak Alice_solar_260044ac:

    # Max_00 "Ясно. Ну, в другой раз значит..."
    Max_00 "Chápem. Tak teda inokedy..."

# game/dialogues/alicetalk.rpy:2630
translate slovak Alice_solar_596d8ab7:

    # Max_09 "{m}Такой крем наверняка можно найти в интернет-магазине. Да и прежде чем пытаться поприставать к сестрёнке таким образом, стоит обзавестись одеждой полегче.{/m}"
    Max_09 "{m}Takýto krém pravdepodobne nájdem v internetovom obchode. A predtým, ako sa pokúsim takto obťažovať svoju sestru, je dobré zaobstarať si nejaké ľahšie oblečenie.{/m}"

# game/dialogues/alicetalk.rpy:2628
translate slovak Alice_solar_f917ca15:

    # Max_07 "{m}Прежде чем пытаться поприставать к сестрёнке таким образом, стоит обзавестись одеждой полегче.{/m}"
    Max_07 "{m}Predtým, ako sa pokúsite takto obťažovať svoju sestru, je dobré zaobstarať si nejaké ľahšie oblečenie.{/m}"

# game/dialogues/alicetalk.rpy:2633
translate slovak Alice_solar_a419850a:

    # Max_07 "{m}Крем почти закончился. Нужно купить ещё.{/m}"
    Max_07 "{m}Opaľovací krém takmer došiel. Musím ho dokúpiť.{/m}"

# game/dialogues/alicetalk.rpy:2638
translate slovak Alice_solar_22c9ef31:

    # Alice_03 "Если у тебя есть крем, то давай, раз тебе делать нечего."
    Alice_03 "Ak máš krém, choď do toho, keďže nemáš nič lepšie na práci."

# game/dialogues/alicetalk.rpy:2639
translate slovak Alice_solar_93baf4d9:

    # Max_01 "Ложись на живот тогда..."
    Max_01 "Tak si ľahni na brucho..."

# game/dialogues/alicetalk.rpy:2648
translate slovak Alice_solar_type_choice_327b0489:

    # Alice_07 "Эти шезлонги всем хороши, но на животе загорать не получается. Приходится коврик для йоги использовать..." nointeract
    Alice_07 "Tieto ležadlá sú vhodné na všetko, ale nemôžem sa opaľovať na bruchu. Musím používať podložku na jogu..." nointeract

# game/dialogues/alicetalk.rpy:2658
translate slovak Alice_solar_type_choice_a17bff06:

    # Max_01 "{m}Так, хорошенько намажем эти стройные ножки...{/m}"
    Max_01 "{m}Poďme teraz tieto štíhle nohy poriadne natrieť...{/m}"

# game/dialogues/alicetalk.rpy:2662
translate slovak Alice_solar_type_choice_c137fcb4:

    # Max_01 "{m}Теперь плечи и совсем немного шею...{/m}" nointeract
    Max_01 "{m}Teraz ramená a časť krku...{/m}" nointeract

# game/dialogues/alicetalk.rpy:2672
translate slovak Alice_solar_type_choice_ddface62:

    # Max_03 "{m}И закончим, хорошенько намазав всю её спину...{/m}"
    Max_03 "{m}Dokončím to tým, že jej poriadne natriem chrbát...{/m}"

# game/dialogues/alicetalk.rpy:2675
translate slovak Alice_solar_type_choice_f13e59c6:

    # Alice_04 "Спасибо, Макс! На сегодня достаточно. У тебя очень неплохо получается, а если поучишься, может стать ещё лучше!"
    Alice_04 "Ďakujem, Max! To zatiaľ stačí. Vedieš si veľmi dobre, a ak sa budeš učiť, môžeš sa ešte zlepšiť!"

# game/dialogues/alicetalk.rpy:2676
translate slovak Alice_solar_type_choice_acc696ab:

    # Max_04 "Да не за что, обращайся!"
    Max_04 "Nemáš za čo!"

# game/dialogues/alicetalk.rpy:2682
translate slovak Alice_solar_type_choice_14b2ab6d:

    # Max_07 "{m}В чём-то Алиса права, поучиться этому, пожалуй, стоит.{/m}"
    Max_07 "{m}Alica má pravdu; pravdepodobne sa to oplatí naučiť.{/m}"

# game/dialogues/alicetalk.rpy:2691
translate slovak Alice_solar_type_choice_69129f17:

    # Alice_03 "Спасибо, Макс. Так намного лучше..."
    Alice_03 "Ďakujem, Max. Je to oveľa lepšie..."

# game/dialogues/alicetalk.rpy:2692
translate slovak Alice_solar_type_choice_ca46ce2b:

    # Max_04 "Обращайся, если что..."
    Max_04 "Ak budeš niečo potrebovať, môžeš ma kontaktovať..."

# game/dialogues/alicetalk.rpy:2699
translate slovak Alice_solar_type_choice_68a6f9b5:

    # Max_10 "{m}Ну вот, крем закончился. Надо ещё купить.{/m}"
    Max_10 "{m}No, to je všetko, krém došiel. Musím kúpiť ďalší.{/m}"

# game/dialogues/alicetalk.rpy:2704
translate slovak Alice_solar_type_choice_61f00467:

    # Max_08 "{m}Осталось мало крема, в следующий раз может не хватить, лучше купить заранее.{/m}"
    Max_08 "{m}Opaľovacieho krému už nie je veľa, nabudúce by nemusel stačiť, radšej ho dokúpim v predstihu.{/m}"

# game/dialogues/alicetalk.rpy:2715
translate slovak Alice_solar_type_choice_0887200d:

    # Alice_00 "Ну что же ты, Макс... Эх, только настроилась..."
    Alice_00 "No tak, Max... Eh, len sa dostávam do nálady..."

# game/dialogues/alicetalk.rpy:2733
translate slovak massage_sunscreen_2b064227:

    # Alice_07 "Макс, ты делаешь успехи! Ещё немного попрактикуешься, и к тебе будет сложно записаться на приём!"
    Alice_07 "Max, robíš pokroky! S trochou praxe bude ťažké dohodnúť si s tebou stretnutie!"

# game/dialogues/alicetalk.rpy:2734
translate slovak massage_sunscreen_e885dc43:

    # Max_03 "Да пустяки, обращайся!"
    Max_03 "To nič nie je!"

# game/dialogues/alicetalk.rpy:2735
translate slovak massage_sunscreen_ea601673:

    # Alice_04 "Ладно, хватит на сегодня, Макс. И... спасибо!"
    Alice_04 "Dobre, pre dnešok to stačí, Max.  Ďakujem!"

# game/dialogues/alicetalk.rpy:2736
translate slovak massage_sunscreen_ca31b759:

    # Max_05 "Не за что! Всегда рад..."
    Max_05 "Nemáš za čo! Vždy je potešením..."

# game/dialogues/alicetalk.rpy:2743
translate slovak massage_sunscreen_f13e59c6:

    # Alice_04 "Спасибо, Макс! На сегодня достаточно. У тебя очень неплохо получается, а если поучишься, может стать ещё лучше!"
    Alice_04 "Ďakujem, Max! To zatiaľ stačí. Vedieš si veľmi dobre, a ak sa budeš učiť, môžeš sa ešte zlepšiť!"

