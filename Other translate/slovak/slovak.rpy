    new "Je to hrozba?"

    # game/dialogues/erictalk.rpy:71
    old "Хорошо, Эрик!"
    new "Dobre, Erik!"

    # game/dialogues/erictalk.rpy:159
    old "{i}сесть на кровать{/i}"
    new "{i}sadnúť na posteľ{/i}"

    # game/dialogues/erictalk.rpy:174
