    new "Sekretárka"

    # game/dialogues/breakfast.rpy:754
    old "{i}промолчать{/i}"
    new "{i}zostať ticho{/i}"

    # game/dialogues/breakfast.rpy:757
    old "В общем, секретарша..."
    new "Každopádne, sekretárka..."

    # game/dialogues/breakfast.rpy:764
