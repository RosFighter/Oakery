    new "Je to blbec!"

    # game/dialogues/breakfast.rpy:708
    old "Извини, мам..."
    new "Prepáč, mami..."

    # game/dialogues/breakfast.rpy:754
    old "Горничной Эрика!"
    new "Erikova slúžka!"

    # game/dialogues/breakfast.rpy:754
