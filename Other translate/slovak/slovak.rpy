# game/dialogues/alicetalk.rpy:3666
translate slovak alice_sorry_gifts_middle_b4a32aa4_1:

    # Max_10 "Да я же случайно оказался около душа..."
    Max_10 "Náhodou som sa ocitol v blízkosti sprchy..."

# game/dialogues/alicetalk.rpy:3667
translate slovak alice_sorry_gifts_middle_14c24df1:

